<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.sapient.oms.entity.Store" table = "store">
      <meta attribute = "class-description">
         This class contains the store blueprint. 
      </meta>
      <id name = "storeId" type = "int" column = "id">
         <generator class="native"/>
      </id>
      <property name = "storeName" column = "storeName" type = "string"/>
      <property name = "owner" column = "owner" type = "string"/>
      <property name = "contact" column = "contact" type = "int"/>
      <!--property name = "locationId" column = "locationId" type = "int"/-->
    
   </class>
</hibernate-mapping>